{
    "manifest_version": 3,
    "name": "My YouTube Content Script Extension",
    "version": "1.0.0",
    "description": "This extension injects a content script into YouTube video pages.",
    "permissions": [
      "nativeMessaging",
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/watch*"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "background": {
      "service_worker": "service-worker.js"
    }
  }
  